{
  "name": "advanced-discord-bot",
  "version": "1.0.0",
  "description": "An advanced discord bot with a few stuff here and there",
  "main": "src/index.ts",
  "keywords": [
    "discord",
    "discord.js",
    "discord.ts",
    "advanced-discord-bot",
    "discord-bot",
    "discord-bot-games",
    "github-discord-bot"
  ],
  "engines": {
    "node": "18.3.0"
  },
  "scripts": {
    "start": "npx ts-node .",
    "build": "tsc --build",
    "prod": "tsc --build && node ./dist/src/index.js",
    "dev": "ts-node-dev --respawn --transpile-only --poll ./src/index.ts",
    "register-commands": "ts-node ./scripts/registerCommands.ts",
    "delete-commands": "ts-node ./scripts/deleteCommands.ts"
  },
  "author": "notbeer",
  "license": "GNU",
  "dependencies": {
    "axios": "^1.1.3",
    "canvas": "^2.10.2",
    "chalk": "^4.1.2",
    "discord.js": "^14.18.0",
    "dotenv": "^12.0.3",
    "i18n": "^0.15.1",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@types/i18n": "^0.13.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3"
  }
}
