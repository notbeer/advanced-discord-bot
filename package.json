{
  "name": "advanced-discord-bot",
  "version": "1.0.1",
  "description": "An advanced discord bot with a few stuff here and there",
  "main": "dist/src/index.js",
  "keywords": [
    "discord",
    "discord.js",
    "discord.ts",
    "advanced-discord-bot",
    "discord-bot",
    "discord-bot-games",
    "github-discord-bot"
  ],
  "engines": {
    "node": "18.3.0"
  },
  "scripts": {
    "start": "node .",
    "build": "tsc --build",
    "prod": "tsc --build && node ./dist/src/index.js",
    "dev": "ts-node-dev --respawn --transpile-only --poll ./src/bot.ts",
    "deploy-commands": "ts-node ./scripts/deploy-commands.ts",
    "delete-commands": "ts-node ./scripts/delete-commands.ts"
  },
  "author": "notbeer",
  "license": "GNU",
  "dependencies": {
    "canvas": "^3.1.0",
    "chalk": "^4.1.2",
    "discord.js": "^14.18.0",
    "dotenv": "^12.0.4",
    "i18n": "^0.15.1",
    "moment": "^2.30.1",
    "mongoose": "^8.10.1"
  },
  "devDependencies": {
    "@types/i18n": "^0.13.5",
    "@types/node": "^22.13.8",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2"
  }
}
